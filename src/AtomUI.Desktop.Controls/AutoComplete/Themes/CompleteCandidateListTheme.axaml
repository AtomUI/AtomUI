<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:CompleteCandidateList}" TargetType="atom:CompleteCandidateList">
        <ControlTheme.BasedOn>
            <atom:ListBoxTheme TargetType="atom:CompleteCandidateList" />
        </ControlTheme.BasedOn>
        
        <Setter Property="IsBorderless" Value="True"/>
        <Setter Property="IsShowSelectedIndicator" Value="False"/>
        <Setter Property="ItemSelectedBg" Value="{DynamicResource {x:Static atom:ListBoxTokenKey.ItemHoverBgColor}}" />
        
    </ControlTheme>
</ResourceDictionary>