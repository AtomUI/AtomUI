            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_Upload}}"
                              ItemKey="{x:Static viewmodels:UploadViewModel.ID}" />
        </atom:NavMenuNode>

