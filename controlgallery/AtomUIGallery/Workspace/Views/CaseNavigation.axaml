            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_InfoFlyout}}"
                              ItemKey="{x:Static viewmodels:InfoFlyoutViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_List}}"
