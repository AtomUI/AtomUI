<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:CompactSpace}" TargetType="atom:CompactSpace">
        <Setter Property="Template">
            <ControlTemplate>
                <Grid Name="{x:Static atom:CompactSpaceThemeConstants.ContentLayoutPart}"/>
            </ControlTemplate>
        </Setter>
        
        <Setter Property="ClipToBounds" Value="False"/>
    </ControlTheme>
</ResourceDictionary>