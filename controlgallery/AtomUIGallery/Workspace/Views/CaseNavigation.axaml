                              ItemKey="{x:Static viewmodels:SpinViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Feedback_Watermark}}"
                              ItemKey="{x:Static viewmodels:WatermarkViewModel.ID}" />
