<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    x:ClassModifier="internal"
                    xmlns:atom="https://atomui.net"
                    xmlns:themes="using:AtomUI.Desktop.Controls.Themes">
    <themes:ListTheme x:Key="{x:Type atom:List}" TargetType="atom:List" />
    <themes:GroupableListViewTheme x:Key="{x:Type atom:GroupableListView}" TargetType="atom:GroupableListView" />
    <themes:ListItemTheme x:Key="{x:Type atom:ListItem}" TargetType="atom:ListItem" />
</ResourceDictionary>