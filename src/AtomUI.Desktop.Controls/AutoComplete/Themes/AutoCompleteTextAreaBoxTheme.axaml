<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:AutoCompleteTextAreaBox}" TargetType="atom:AutoCompleteTextAreaBox">
        <ControlTheme.BasedOn>
            <atom:TextAreaTheme TargetType="{x:Type atom:AutoCompleteTextAreaBox}" />
        </ControlTheme.BasedOn>
    </ControlTheme>
</ResourceDictionary>