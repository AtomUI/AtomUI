<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AtomUIGallery.Workspace.Views.CaseNavigation"
             xmlns:atom="https://atomui.net"
             xmlns:antdicons="https://atomui.net/icons/antdesign"
             xmlns:viewmodels="using:AtomUIGallery.ShowCases.ViewModels"
             xmlns:lang="using:AtomUIGallery.Workspace.Localization.CaseNavigationLang">
    <atom:NavMenu Mode="Inline" Width="260" Name="ShowCaseNavMenu"
                  atom:ScrollViewer.IsLiteMode="True"
                  ScrollViewer.AllowAutoHide="True">
        <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.General}}"
                          Icon="{antdicons:AntDesignIconProvider Kind=WindowsOutlined}">
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.General_AboutUs}}"
                              ItemKey="{x:Static viewmodels:AboutUsViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.General_OsInfo}}"
                              ItemKey="{x:Static viewmodels:OsInfoViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.General_Palette}}"
                              ItemKey="{x:Static viewmodels:PaletteViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.General_Icons}}"
                              ItemKey="{x:Static viewmodels:IconViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.General_Button}}"
                              ItemKey="{x:Static viewmodels:ButtonViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.General_SplitButton}}"
                              ItemKey="{x:Static viewmodels:SplitButtonViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.General_Separator}}"
                              ItemKey="{x:Static viewmodels:SeparatorViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.General_CustomizeTheme}}"
                              ItemKey="{x:Static viewmodels:CustomizeThemeViewModel.ID}" />
        </atom:NavMenuNode>
        
        <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Layout}}"
                          Icon="{antdicons:AntDesignIconProvider Kind=LayoutOutlined}">
            <atom:NavMenuNode ItemKey="{x:Static viewmodels:BoxPanelViewModel.ID}">
                <atom:NavMenuNode.HeaderTemplate>
                    <DataTemplate>
                        <atom:FlexPanel Direction="Row"
                                        AlignItems="Center"
                                        JustifyContent="FlexStart"
                                        HorizontalAlignment="Stretch">
                            <TextBlock atom:Flex.Grow="1"
                                       Text="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Layout_BoxPanel}}"
                                       VerticalAlignment="Center" />
                            <atom:Tag TagColor="red"
                                      Bordered="False"
                                      Padding="4,0"
                                      VerticalAlignment="Center"
                                      Margin="8,0,0,0"
                                      TagText="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Layout_BoxPanel_DeprecatedTag}}" />
                        </atom:FlexPanel>
                    </DataTemplate>
                </atom:NavMenuNode.HeaderTemplate>
            </atom:NavMenuNode>
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Layout_FlexPanel}}"
                              ItemKey="{x:Static viewmodels:FlexPanelViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Layout_Grid}}"
                              ItemKey="{x:Static viewmodels:GridViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Layout_Space}}"
                              ItemKey="{x:Static viewmodels:SpaceViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Layout_Splitter}}"
                              ItemKey="{x:Static viewmodels:SplitterViewModel.ID}" />
        </atom:NavMenuNode>

        <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Navigation}}"
                          Icon="{antdicons:AntDesignIconProvider Kind=MenuOutlined}">
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Navigation_Breadcrumb}}"
                              ItemKey="{x:Static viewmodels:BreadcrumbViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Navigation_ButtonSpinner}}"
                              ItemKey="{x:Static viewmodels:ButtonSpinnerViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Navigation_ComboBox}}"
                              ItemKey="{x:Static viewmodels:ComboBoxViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Navigation_DropdownButton}}"
                              ItemKey="{x:Static viewmodels:DropdownButtonViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Navigation_Menu}}"
                              ItemKey="{x:Static viewmodels:MenuViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Navigation_Pagination}}"
                              ItemKey="{x:Static viewmodels:PaginationViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Navigation_Steps}}"
                              ItemKey="{x:Static viewmodels:StepsViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Navigation_TabControl}}"
                              ItemKey="{x:Static viewmodels:TabControlViewModel.ID}" />
        </atom:NavMenuNode>

        <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry}}"
                          Icon="{antdicons:AntDesignIconProvider Kind=FormOutlined}">
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_AutoComplete}}"
                              ItemKey="{x:Static viewmodels:AutoCompleteViewModel.ID}" />
