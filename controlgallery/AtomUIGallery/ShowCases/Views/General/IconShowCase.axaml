<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AtomUIGallery.ShowCases.Views.IconShowCase"
             xmlns:atom="https://atomui.net"
             xmlns:gallery="https://atomui.net/oss-controls/gallery">
    <atom:TabControl>
        <atom:TabItem Header="Outlined">
            <atom:ScrollViewer>
                <gallery:IconGallery IconThemeType="Outlined" />
            </atom:ScrollViewer>
        </atom:TabItem>
        <atom:TabItem Header="Filled">
            <atom:ScrollViewer>
                <gallery:IconGallery IconThemeType="Filled" />
            </atom:ScrollViewer>
        </atom:TabItem>
        <atom:TabItem Header="Two Tone">
            <atom:ScrollViewer>
                <gallery:IconGallery IconThemeType="TwoTone" />
            </atom:ScrollViewer>
        </atom:TabItem>
    </atom:TabControl>
</UserControl>