<ResourceDictionary
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:atom="https://atomui.net"
    x:ClassModifier="internal">
    <ControlTheme x:Key="{x:Type atom:Upload}" TargetType="atom:Upload">
        <Setter Property="Template">
            <ControlTemplate>
                <ResourceInclude Source="UploadTheme.axaml" />
            </ControlTemplate>
        </Setter>
    </ControlTheme>
</ResourceDictionary>