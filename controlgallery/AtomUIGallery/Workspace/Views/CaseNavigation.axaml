            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_ColorPicker}}"
                              ItemKey="{x:Static viewmodels:ColorPickerViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_DatePicker}}"
                              ItemKey="{x:Static viewmodels:DatePickerViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_TimePicker}}"
                              ItemKey="{x:Static viewmodels:TimePickerViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_LineEdit}}"
                              ItemKey="{x:Static viewmodels:LineEditViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_Mentions}}"
                              ItemKey="{x:Static viewmodels:MentionsViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_NumberUpDown}}"
                              ItemKey="{x:Static viewmodels:NumberUpDownViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_RadioButton}}"
                              ItemKey="{x:Static viewmodels:RadioButtonViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_Rate}}"
                              ItemKey="{x:Static viewmodels:RateViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_Select}}"
                              ItemKey="{x:Static viewmodels:SelectViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_Slider}}"
                              ItemKey="{x:Static viewmodels:SliderViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataEntry_ToggleSwitch}}"
