            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_Tooltip}}"
                              ItemKey="{x:Static viewmodels:TooltipViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_Tag}}"
                              ItemKey="{x:Static viewmodels:TagViewModel.ID}" />
        </atom:NavMenuNode>

        <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Feedback}}"
                          Icon="{antdicons:AntDesignIconProvider Kind=UserSwitchOutlined}">
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Feedback_Alert}}"
                              ItemKey="{x:Static viewmodels:AlertViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Feedback_Drawer}}"
                              ItemKey="{x:Static viewmodels:DrawerViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Feedback_Message}}"
                              ItemKey="{x:Static viewmodels:MessageViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Feedback_Modal}}"
                              ItemKey="{x:Static viewmodels:ModalViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Feedback_Notification}}"
                              ItemKey="{x:Static viewmodels:NotificationViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.Feedback_PopupConfirm}}"
