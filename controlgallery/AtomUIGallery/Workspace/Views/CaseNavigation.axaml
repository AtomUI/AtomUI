            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_Avatar}}"
                              ItemKey="{x:Static viewmodels:AvatarViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_Badge}}"
                              ItemKey="{x:Static viewmodels:BadgeViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_Calendar}}"
                              ItemKey="{x:Static viewmodels:CalendarViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_Card}}"
                              ItemKey="{x:Static viewmodels:CardViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_Carousel}}"
                              ItemKey="{x:Static viewmodels:CarouselViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_Collapse}}"
                              ItemKey="{x:Static viewmodels:CollapseViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_Descriptions}}"
                              ItemKey="{x:Static viewmodels:DescriptionsViewModel.ID}" />
            <atom:NavMenuNode Header="{DynamicResource {x:Static lang:CaseNavigationLangResourceKey.DataDisplay_DataGrid}}"
